@model List<MVCProfessionals_1july2021.Models.EmployeeModel>
@{
    ViewBag.Title = "SendEmployeeInfo13";
}

<h2>SendEmployeeInfo13</h2>

<table>
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
@foreach (var item in Model)
{
    <tr>
        <td>@item.EmpId</td>
        <td>@item.EmpName</td>
        <td>@item.EmpSalary</td>
    </tr>
}

</table>