@model PagedList<EmployeeModel>
@{
    ViewBag.Title = "Index";
}
@using ADO_DotNetExample.Models
@using PagedList
@using PagedList.Mvc
<h2>Index</h2>

    <div class="panel panel-heading">
    <table class="table table-bordered table-striped">
        <tr>
            <th>EmpId</th>
            <th>EmpName</th>
            <th>EmpSalary</th>
        </tr>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
        }
    </table>
        @Html.PagedListPager(Model,PageNumber=>Url.Action("Index","Home",new {PageNumber}))
</div>